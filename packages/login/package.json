{
  "name": "aelf-web-login",
  "keywords": [
    "antd",
    "portkey"
  ],
  "description": "",
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AElf-devops/aelf-web-login.git",
    "directory": "packages/login"
  },
  "version": "0.0.11-alpha",
  "main": "dist/index.js",
  "types": "dist/_types/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "start": "tsc --watch",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:md": "remark . -f -q",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:source": "rollup --silent --config rollup.config.js",
    "build": "npm run build:clean; npm run build:types & npm run build:source",
    "dev": "rollup --config rollup.config.js --watch",
    "release": "yarn version && yarn build",
    "pub": "npm publish"
  },
  "dependencies": {
    "@aelf-react/core": "^0.1.19",
    "aelf-sdk": "^3.2.40",
    "@portkey/did-ui-react": "0.0.1-alpha.8.1"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less": "^4.1.3"
  }
}
